{
  "name": "kazkiueda_com",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "bunx oxlint --fix src/ --deny-warnings && bun run type-check && bun run format",
    "type-check": "tsc --noEmit",
    "lint": "bunx oxlint src/",
    "lint:fix": "bunx oxlint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:vrt": "lost-pixel --baseline-dir=./lost-pixel/baseline --current-dir=./lost-pixel/current --difference-dir=./lost-pixel/difference --concurrency=2 --fail-on-difference --github.sha=$GITHUB_SHA --github.repository=$GITHUB_REPOSITORY --github.repository-owner=$GITHUB_REPOSITORY_OWNER --github.pr-number=$GITHUB_PR_NUMBER",
    "test:vrt:update": "lost-pixel update --baseline-dir=./lost-pixel/baseline --current-dir=./lost-pixel/current --difference-dir=./lost-pixel/difference",
    "test:vrt:approve": "lost-pixel approve --baseline-dir=./lost-pixel/baseline --current-dir=./lost-pixel/current --difference-dir=./lost-pixel/difference",
    "test:vrt:ci": "lost-pixel --fail-on-difference --baseline-dir=./lost-pixel/baseline --current-dir=./lost-pixel/current --difference-dir=./lost-pixel/difference --concurrency=2 --github.sha=$GITHUB_SHA --github.repository=$GITHUB_REPOSITORY --github.repository-owner=$GITHUB_REPOSITORY_OWNER --github.pr-number=$GITHUB_PR_NUMBER",
    "test:vrt:baseline": "lost-pixel --generate-only --baseline-dir=./lost-pixel/baseline --current-dir=./lost-pixel/current --difference-dir=./lost-pixel/difference",
    "test:vrt:local": "lost-pixel --fail-on-difference=false --baseline-dir=./lost-pixel/baseline --current-dir=./lost-pixel/current --difference-dir=./lost-pixel/difference",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "overrides": {
    "@playwright/test": "^1.53.1",
    "playwright": "^1.53.1",
