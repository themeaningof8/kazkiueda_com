{
  "threshold": 3.0,
  "reporters": ["console", "html"],
  "ignore": [
    "node_modules/**",
    ".next/**",
    "dist/**",
    "build/**",
    "coverage/**",
    "**/*.min.js",
    "**/payload-types.ts",
    "**/__tests__/**",
    "**/tests/**",
    "**/admin/**"
  ],
  "format": ["typescript", "javascript", "tsx", "jsx"],
  "minLines": 5,
  "minTokens": 50,
  "output": "./reports/jscpd"
}
