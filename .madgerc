{
  "fontSize": "10px",
  "detectiveOptions": {
    "ts": {
      "skipTypeImports": true
    },
    "tsx": {
      "skipTypeImports": true
    }
  },
  "fileExtensions": ["ts", "tsx", "js", "jsx"],
  "excludeRegExp": [
    "node_modules/",
    ".next/",
    "dist/",
    "build/",
    "coverage/"
  ],
  "tsConfig": "tsconfig.json"
}
